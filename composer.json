{
    "name": "uga/twig2pdf-bundle",
    "type": "symfony-bundle",
    "description": "Twig2pdf for Symfony 2.8 LTS ",
    "keywords": ["pdf", "tcpdf", "html2pdf"],
    "homepage": "https://github.com/nfouka/HTML2PDF_spipu",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Fouka",
            "email": "nadir.fouka@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.0",
        "spipu/html2pdf": "~4.5",
        "uga/twig2pdf-bundle": "@dev"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~3.0"
    },
    "autoload": {
        "psr-0": { "UGA\\Html2PDFBundle": "" }
    },
    "target-dir": "UGA/Html2pdfBundle"
}
